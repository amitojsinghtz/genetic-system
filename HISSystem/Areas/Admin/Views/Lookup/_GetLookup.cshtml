@model Data.Models.Lookup
@using Data.Models

<form @*action="/admin/lookup/GetLookup" method="post" id="editLookUp"*@ >
    <div class="prtm-content">
        <div class="row mrgn-b-sm">
            <div class="col-md-4 col-lg-4">
                <input type="button" name="submit" onclick="EditLookUp();" value="Update" class="btn btn-warning sbmtBtn" />
                <input type="button" name="submit" value="Back" class="btn btn-info" />
            </div>
            <div class="col-md-4 col-lg-4">
            </div>
            <div class="col-md-4 col-lg-4">
            </div>
        </div>
        <div class="form-style">
            <div class="row">
                <div class="col-md-12 col-lg-12">
                    <div class="prtm-block pos-relative">
                        <div class="row">
                            <div class="col-md-8 col-lg-8">
                                <div class="row mrgn-l-md">
                                    @Html.HiddenFor(m => m.ID)
                                    <div class="form-group">
                                        <label for="Type">Type</label>
                                        @Html.TextBoxFor(m => m.Type, null, new { @Class = "form-control", @readonly = true })
                                    </div>
                                    <div class="form-group">
                                        <label for="TypeID">TypeID</label>
                                        @Html.TextBoxFor(m => m.TypeID, null, new { @Class = "form-control", @readonly = true })
                                    </div>
                                    <div class="form-group">
                                        <label for="Name">Name</label>
                                        @Html.TextBoxFor(m => m.Name, null, new { @Class = "form-control", autocomplete = "off" })
                                    </div>
                                    <div class="form-group">
                                        <label for="IsActive">Is Active</label>
                                        @Html.CheckBoxFor(m => m.IsActive, new { @checked = "checked", style = "right:0px !important" })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
