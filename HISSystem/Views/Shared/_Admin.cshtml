    <script>

        $(document).on('click', '#sbmtChngPssBtn', function () {
            var oldPass = $("#inputoldPassword").val();
            var newPass = $("#inputNewPassword").val();
            var confPass = $("#inputConfPassword").val();

            if (oldPass == null || oldPass == "") {
                document.getElementById('reqOldPass').style.display = 'block';
                return false;
            }
            if (newPass == null || newPass == "") {
                document.getElementById('reqNewPass').style.display = 'block';
                return false;
            }
            if (confPass == null || confPass == "") {
                document.getElementById('reqConfPass').style.display = 'block';
                return false;
            }
            if (newPass != confPass) {
                document.getElementById('matchPass').style.display = 'block';
                return false;
            }
            var changePass = new FormData();
            changePass.append("userName", $("#usrName").val());
            changePass.append("oldPass", $("#inputoldPassword").val());
            changePass.append("newPass", $("#inputNewPassword").val());
            for (var pair of changePass.entries()) {
                console.log(pair[0] + ', ' + pair[1]);
            }
            $.ajax({
                type: 'POST',
                url: "/account/changePassword",
                data: changePass,
                dataType: "json",
                processData: false,
                contentType: false,
                success: function (response) {
                    if (response == false)
                        toastr.error('Wrong credentials!')
                    else {
                        toastr.success('Success!')
                    }
                },
                error: function (xhr, status) {
                    toastr.error('Something went wrong');
                }
            });
        });
    </script>
</body>
</html>
